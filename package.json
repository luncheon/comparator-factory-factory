{
  "name": "comparing",
  "version": "0.0.0",
  "description": "Create a comparison function to be used for sorting arrays.",
  "main": "index",
  "module": "index.mjs",
  "types": "index.d.ts",
  "jsdelivr": "index.iife.min.js",
  "unpkg": "index.iife.min.js",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "index.iife.min.js",
    "src"
  ],
  "keywords": [
    "compare",
    "comparator",
    "comparison",
    "compare-func",
    "array",
    "sort",
    "intl"
  ],
  "license": "WTFPL",
  "repository": "luncheon/comparing",
  "scripts": {
    "clean": "rimraf index.js index.mjs index.d.ts index.iife.min.js coverage",
    "build": "npm run clean && npm run build:es && npm run build:cjs && npm run build:iife && npm test",
    "build:es": "tsc -p . && node -e \"require('fs').renameSync('index.js','index.mjs')\"",
    "build:cjs": "rollup -i index.mjs -f cjs -n comparing -o index.js",
    "build:iife": "rollup -i index.mjs -f iife -n comparing | uglifyjs -m -c join_vars=true -o index.iife.min.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.1",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.22"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
