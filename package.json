{
  "name": "comparing",
  "version": "0.0.0",
  "description": "Create a comparison function to be used for sorting arrays.",
  "main": "index",
  "module": "index.mjs",
  "types": "index.d.ts",
  "keywords": [],
  "license": "WTFPL",
  "repository": "luncheon/comparing",
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "tsc -p . -m es2015 --outDir ._es && node -e \"require('fs').renameSync('._es/index.js','index.mjs')\"",
    "build:cjs": "tsc -p . -m commonjs -d",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
